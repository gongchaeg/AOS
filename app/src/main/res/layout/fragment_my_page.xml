<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/peeka_beige">

    <ImageView
        android:id="@+id/iv_my_page_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="22dp"
        android:src="@drawable/ic_peekabook_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/iv_my_page_rect"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="31dp"
        android:background="@drawable/shape_white30fill_redlines_0_rect"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_my_page_logo">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/iv_my_page_image"
            android:src="@drawable/image"
            android:layout_width="64dp"
            android:layout_height="0dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="16.5dp"
            app:layout_constraintDimensionRatio="1"
            android:layout_marginBottom="19dp"
            app:layout_constraintBottom_toBottomOf="@id/iv_my_page_rect"
            app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"
            app:layout_constraintTop_toTopOf="@id/iv_my_page_rect" />

        <TextView
            android:id="@+id/tv_my_page_name"
            style="@style/H1Bd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:layout_marginTop="21dp"
            android:text="문수빈"
            android:textColor="@color/peeka_red"
            app:layout_constraintStart_toEndOf="@id/iv_my_page_image"
            app:layout_constraintTop_toTopOf="@id/iv_my_page_rect" />

        <ImageView
            android:id="@+id/iv_my_page_edit"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginTop="8dp"
            android:layout_marginStart="28dp"
            android:background="@null"
            android:src="@drawable/ic_my_page_edit"
            app:layout_constraintDimensionRatio="1"
            app:layout_constraintStart_toStartOf="@id/tv_my_page_name"
            app:layout_constraintTop_toTopOf="@id/iv_my_page_rect"/>

        <TextView
            style="@style/S3Md"
            android:layout_width="212dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:textColor="@color/peeka_red"
            android:maxLines="2"
            tools:text="안녕하세요. 저는 한 줄 소개입니다.  "
            app:layout_constraintStart_toStartOf="@id/tv_my_page_name"
            app:layout_constraintTop_toBottomOf="@id/tv_my_page_name" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/tv_my_page_1"
        style="@style/H2Md"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="27dp"
        android:text="알림 설정"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"
        app:layout_constraintTop_toBottomOf="@id/iv_my_page_rect" />

    <View
        android:id="@+id/view_my_page_1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="15dp"
        android:background="@color/peeka_g_1"
        app:layout_constraintTop_toBottomOf="@id/tv_my_page_1" />

    <TextView
        android:id="@+id/tv_my_page_2"
        style="@style/H2Md"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="개인정보 보호 정책, 서비스 이용 약관"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"
        app:layout_constraintTop_toBottomOf="@id/view_my_page_1" />

    <View
        android:id="@+id/view_my_page_2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="15dp"
        android:background="@color/peeka_g_1"
        app:layout_constraintTop_toBottomOf="@id/tv_my_page_2"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"/>

    <TextView
        android:id="@+id/tv_my_page_3"
        style="@style/H2Md"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="문의하기"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"
        app:layout_constraintTop_toBottomOf="@id/view_my_page_2" />

    <View
        android:id="@+id/view_my_page_3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="15dp"
        android:background="@color/peeka_g_1"
        app:layout_constraintTop_toBottomOf="@id/tv_my_page_3"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"/>

    <TextView
        android:id="@+id/tv_my_page_4"
        style="@style/H2Md"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="개발자 정보"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"
        app:layout_constraintTop_toBottomOf="@id/view_my_page_3" />

    <View
        android:id="@+id/view_my_page_4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="15dp"
        android:background="@color/peeka_g_1"
        app:layout_constraintTop_toBottomOf="@id/tv_my_page_4"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"/>

    <TextView
        android:id="@+id/tv_my_page_5"
        style="@style/H2Md"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="로그아웃"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"
        app:layout_constraintTop_toBottomOf="@id/view_my_page_4" />

    <View
        android:id="@+id/view_my_page_5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="15dp"
        android:background="@color/peeka_g_1"
        app:layout_constraintTop_toBottomOf="@id/tv_my_page_5"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"/>

    <TextView
        android:id="@+id/tv_my_page_6"
        style="@style/H2Md"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="서비스 탈퇴하기"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"
        app:layout_constraintTop_toBottomOf="@id/view_my_page_5" />

    <View
        android:id="@+id/view_my_page_6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="15dp"
        android:background="@color/peeka_g_1"
        app:layout_constraintTop_toBottomOf="@id/tv_my_page_6"
        app:layout_constraintStart_toStartOf="@id/iv_my_page_rect"/>


</androidx.constraintlayout.widget.ConstraintLayout>